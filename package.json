{
  "author": {
    "email": "TrejGun+OpenSource@gmail.com",
    "name": "TrejGun"
  },
  "dependencies": {
    "@nestjs/common": "6.9.0",
    "@nestjs/core": "6.9.0",
    "@nestjs/graphql": "6.5.3",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.9.0",
    "@nestjs/typeorm": "6.2.0",
    "apollo-server-express": "2.9.9",
    "dotenv": "8.2.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.12.1",
    "rxjs": "6.5.3",
    "type-graphql": "0.17.5",
    "typeorm": "0.2.20",
    "typeorm-naming-strategies": "1.1.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "6.9.0",
    "@trejgun/eslint-config": "0.0.1",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.7",
    "@types/passport": "1.0.1",
    "@types/passport-jwt": "3.0.3",
    "@types/uuid": "3.4.6",
    "eslint": "6.6.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.0",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.2"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "graphql-based-authorization-for-nestjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrejGun/graphql-based-authorization-for-nestjs"
  },
  "scripts": {
    "build": "tsc",
    "entity:create": "npm run typeorm -- entity:create -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "prebuild": "rm -rf dist/",
    "start": "NODE_ENV=development tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "start:prod": "NODE_ENV=production node .",
    "test": "NODE_ENV=test jest --config jest.config.json",
    "typeorm": "NODE_ENV=development ts-node ./node_modules/.bin/typeorm --config src/ormconfig.ts"
  },
  "title": "GraphQL based authorization for Nest.js",
  "version": "0.0.1"
}
